{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": [
    "./**/*.ts",
    "../nodes/**/*.ts",
    "../credentials/**/*.ts"
  ],
  "exclude": [
    "../node_modules",
    "../dist"
  ]
}
