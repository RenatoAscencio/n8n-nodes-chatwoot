# =============================================================================
# n8n-nodes-chatwoot E2E Testing Environment Variables
# =============================================================================
# Copy this file to .env.local and fill in your values.
# NEVER commit .env.local to version control.
# =============================================================================

# Chatwoot Application API
CHATWOOT_BASE_URL=https://your-chatwoot-instance.com
CHATWOOT_ACCOUNT_ID=1
CHATWOOT_TOKEN=your_chatwoot_api_access_token_here

# Chatwoot Platform API (requires super admin access)
CHATWOOT_PLATFORM_TOKEN=your_platform_api_token_here

# Chatwoot Public API
CHATWOOT_INBOX_IDENTIFIER=your_inbox_identifier_here

# n8n API
N8N_BASE_URL=https://your-n8n-instance.com
N8N_API_KEY=your_n8n_api_key_here
