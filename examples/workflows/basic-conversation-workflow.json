{
  "name": "Chatwoot - List Open Conversations",
  "nodes": [
    {
      "parameters": {},
      "id": "start-node",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "resource": "conversation",
        "operation": "getAll",
        "returnAll": false,
        "limit": 10,
        "filters": {
          "status": "open"
        }
      },
      "id": "chatwoot-list",
      "name": "Get Open Conversations",
      "type": "n8n-nodes-chatwoot.chatwoot",
      "typeVersion": 1,
      "position": [450, 300],
      "credentials": {
        "chatwootApi": {
          "id": "YOUR_CREDENTIAL_ID",
          "name": "Chatwoot API"
        }
      }
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "Get Open Conversations",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "saveDataSuccessExecution": "all",
    "saveDataErrorExecution": "all"
  },
  "staticData": null,
  "tags": []
}
