{
  "name": "@renatoascencio/n8n-nodes-chatwoot",
  "version": "0.3.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "n8n community node for Chatwoot - customer engagement platform for managing conversations, messages, and contacts",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "chatwoot",
    "customer-support",
    "live-chat",
    "helpdesk",
    "messaging",
    "crm"
  ],
  "license": "MIT",
  "homepage": "https://github.com/RenatoAscencio/n8n-nodes-chatwoot",
  "author": {
    "name": "Renato Ascencio",
    "email": "renato_ascencio@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RenatoAscencio/n8n-nodes-chatwoot.git"
  },
  "bugs": {
    "url": "https://github.com/RenatoAscencio/n8n-nodes-chatwoot/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/nodes/Chatwoot/Chatwoot.node.js",
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "build:watch": "tsc --watch",
    "copy:icons": "cp nodes/Chatwoot/*.svg nodes/Chatwoot/*.node.json dist/nodes/Chatwoot/ 2>/dev/null || true",
    "dev": "npm run build:watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ChatwootApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Chatwoot/Chatwoot.node.js",
      "dist/nodes/Chatwoot/ChatwootTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
